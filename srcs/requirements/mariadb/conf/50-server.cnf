[server]

init_file = /etc/mysql/init.sql

[mysqld]

#
# * Basic Settings
#

user                    = root
pid-file                = /run/mysqld/mysqld.pid
basedir                 = /usr
datadir                 = /var/lib/mysql
tmpdir                  = /tmp

bind-address            = mariadb

expire_logs_days        = 10

character-set-server    = utf8mb4
collation-server        = utf8mb4_general_ci

[embebed]
[mariadb]
[mariadb-10.11]
